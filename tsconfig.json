{
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "target": "ES2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "src"
  },
  "include": [
    "./*.js",
    "src/**/*.ts",
    "test/**/*.ts", "src/api/master/category",
  ],
}
